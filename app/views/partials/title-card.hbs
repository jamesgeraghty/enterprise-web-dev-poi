<div class="uk-width-1-1@m  uk-card uk-card-default uk-padding-small uk-visible@m">
    <div class="uk-grid">
        <div class="uk-width-1-5@m">
            <img src="/images/kilcash-sign2.jpg" width="" alt="img" />
        </div>
        <div class="uk-width-expand@m uk-text-left">
            <div class="title">
                {{title}}
            </div>
            <div class="uk-text-muted uk-text-small">
                {{subTitle}}
            </div>
        </div>
        <div class="uk-card uk-card-large uk-text-center uk-padding-remove">
            <a target="_blank" href="https://github.com/jamesgeraghty/enterprise-web-dev-poi">
                <div>
                    <span uk-icon="icon: github; ratio: 3.5"></span>
                </div>
                <div class="uk-width-expand@m uk-text-left">
                    <div class="uk-text-muted uk-text-center" style="font-size:xx-small">Git Repo</div>
                </div>
            </a>
        </div>
    </div>
</div>